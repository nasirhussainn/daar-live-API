{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/auth/(.*)", "dest": "server.js" },
      { "src": "/crud-users/(.*)", "dest": "server.js" },
      { "src": "/property/(.*)", "dest": "server.js" },
      { "src": "/event/(.*)", "dest": "server.js" },
      { "src": "/review/(.*)", "dest": "server.js" },
      { "src": "/realtor/(.*)", "dest": "server.js" },
      { "src": "/property-facilites/(.*)", "dest": "server.js" },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }
  